---
import { Icon } from 'astro-icon/components'

interface Props {
    platform: string
    url: string
}

const { platform, url } = Astro.props
---

<a href={url} aria-label={`Link to my ${platform}`}>
    <Icon
        name={`mdi:${platform.toLowerCase()}`}
        class="w-10 h-10 text-zinc-200 hover:text-zinc-400"
    />
</a>
